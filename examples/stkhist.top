SET NO_XY_SCOLUMNS = 1 3, XY_SCOLUMNS = 1   4   3   2
SET BAR_GRAYNESS =                        0.0 0.4 0.8
SET BAR_WIDTH = 0.6
SET POSITION 12 0
SET WORLD_WINDOW -999 -20 -999 100
